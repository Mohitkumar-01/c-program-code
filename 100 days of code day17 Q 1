#include<stdio.h>
#include<math.h>
int main (){
    int temp,originalnum,n,rem,digit=0,sum=0;
    printf("Enter Any Number: ");
    scanf("%d",&n);
originalnum = n;
temp=n;
    while(temp>0){
       digit++;
        temp /= 10;
    }
    temp =n;
    while(temp>0){
        rem = temp%10;
        sum = sum+pow(rem, digit);
        temp /=10;
    }
    if(originalnum==sum){
        printf("It is a Armstrong Number");
    }
    else{
        printf("It is Not a Armstrong Number");
    }
    return 0;
}