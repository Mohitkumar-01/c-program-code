// Find the sum of array elements
#include<stdio.h>
int main (){
    int i,n,sum=0;
    printf("Enter the number of elements: ");
        scanf("%d",&n);
    int a[n];
   printf("Enter %d Elements:\n",n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
      sum += a[i];
    }
    printf("Sum of array Elements:%d",sum);
    return 0;
}