// Read and print elements of a one-dimensional array.
#include<stdio.h>
int main (){
    int i,n;
    printf("Enter the number of elements: ");
        scanf("%d",&n);
    int a[n];
   printf("Enter %d Elements:\n",n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
     printf("The Elements are:\n");
    for(i=0;i<n;i++){
        printf("%d ",a[i]);
    }
    return 0;
}