//Finding the Prime Number
#include<stdio.h>
int main (){
    int n,i,b=0;
    printf("Enter any number: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        if(n%i==0){
            b++;
        }
    }
    if(b==2){
        printf("It is a Prime Number");
    }
    else{
        printf("It is Not a Prime Number");
    }
    return 0;
}